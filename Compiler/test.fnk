function List(a...){
    return a;
}

var list = List(1, 2, 3);

with list {
    print(this);

    function this:Announce(){
        print("Hi! I am:", this)
    }
}

list:Announce();

thief = List(4, 5, 6)
thief.Announce = list.Announce;

thief:Announce();

anotherThief = List(7, 8, 9);
with anotherThief {
    Announce = list.Announce;
}

anotherThief:Announce();